#include <stdio.h>
#include <conio.h>
#include <stdlib.h>
int menu_display(void);
void hamburger(void);
void spaghetti(void);
void press_any_key(void);
int main(void)
{
int c;
while((c=menu_display()) != 3)
{
switch(c)
{
case 1 : hamburger();
break;
case 2 : spaghetti();
break;
default : break;
}
}
return 0;
}
